{
  "language": "en",
  "dictionaries": [
    "dict"
  ],
  "dictionaryDefinitions": [
    {
      "name": "dict",
      "path": "./dict.txt",
      "addWords": true
    }
  ],
  "ignorePaths": [
    // binaries
    "**/*.a",
    "**/*.dll",
    "**/*.dylib",
    "**/*.jar",
    "**/*.mp3",
    "**/*.node",
    "**/*.ppn",
    "**/*.pv",
    "**/*.so",
    "**/*.wasm",
    "**/*.wav",
    "../../../lib/ios/**/*",

    // ios
    "**/*.xcconfig",
    "**/*.podspec",
    "**/Podfile",
    "**/*.plist",
    "**/contents.xcworkspacedata",
    "**/*.storyboard",
    "**/*.pbxproj",
    "**/*.xcscheme",
    "**/*.m"
  ]
}
